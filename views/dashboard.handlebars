    </div>

    <!-- Earnings (Monthly) Card Example -->
    <div class="col-xl-3 col-md-6 mb-4">
        <div class="card border-left-primary h-100 py-2">
            <div class="card-body">
                <div class="row no-gutters align-items-center">
                    <div class="col mr-2">
                        <div class="text-xs font-weight-bold text-gray-800 text-uppercase mb-1">Ruangan</div>
                        <div class="h5 mb-0 font-weight-bold text-primary">{{ ruangan }}</div>
                    </div>
                    <div class="col-auto">
