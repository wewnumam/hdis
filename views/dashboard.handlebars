</div>


<!-- Content Row -->
<div class="row">

    <!-- Content Column -->
    <div class="col-lg-4 mb-4">

        <!-- Project Card Example -->
        <div class="card mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-gray-800">Pasien</h6>
            </div>
            <div class="card-body">
                <h4 class="small font-weight-bold">Laki-Laki<span class="float-right">{{ pasienLk }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar" role="progressbar" style="width: {{persen pasienLk pasien}}%"
                        aria-valuenow="20" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <h4 class="small font-weight-bold">Perempuan <span class="float-right">{{ pasienPr }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-danger" role="progressbar" style="width: {{persen pasienPr pasien }}%"
                        aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <h4 class="small font-weight-bold">Rawat Jalan <span class="float-right">{{ pasienRj }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-success" role="progressbar" style="width: {{persen pasienRj pasien}}%"
                        aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <h4 class="small font-weight-bold">Rawat Inap <span class="float-right">{{ pasienRi }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-warning" role="progressbar" style="width: {{persen pasienRi pasien}}%"
                        aria-valuenow="80" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>

    <!-- Content Column -->
    <div class="col-lg-4 mb-4">

        <!-- Project Card Example -->
        <div class="card mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-gray-800">Ruangan</h6>
            </div>
            <div class="card-body">
                <h4 class="small font-weight-bold">Terisi<span class="float-right">{{ ruangTerisi }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-success" role="progressbar" style="width: {{persen ruangTerisi ruangan}}%" aria-valuenow="20"
                        aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <h4 class="small font-weight-bold">Kosong <span class="float-right">{{ ruangKosong }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-warning" role="progressbar" style="width: {{persen ruangKosong ruangan }}%"
                        aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>

    <!-- Content Column -->
    <div class="col-lg-4 mb-4">

        <!-- Project Card Example -->
        <div class="card mb-4">
            <div class="card-header py-3">
                <h6 class="m-0 font-weight-bold text-gray-800">Tenaga Medis</h6>
            </div>
            <div class="card-body">
                <h4 class="small font-weight-bold">Laki-Laki<span class="float-right">{{ TMLk }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar" role="progressbar" style="width: {{persen TMLk TM}}%" aria-valuenow="20"
                        aria-valuemin="0" aria-valuemax="100"></div>
                </div>
                <h4 class="small font-weight-bold">Perempuan <span class="float-right">{{ TMPr }}</span></h4>
                <div class="progress mb-4">
                    <div class="progress-bar bg-danger" role="progressbar" style="width: {{persen TMPr TM }}%"
                        aria-valuenow="40" aria-valuemin="0" aria-valuemax="100"></div>
                </div>
            </div>
        </div>
    </div>
</div>